{
  "name": "mehr-it/lara-ext",
  "description": "Collection of helpers and macros for laravel",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Christopher Mieger",
      "email": "mail@mehr-it.info"
    }
  ],
  "autoload": {
    "psr-4": {
      "MehrIt\\LaraExt\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MehrItLaraExtTest\\": "test/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MehrIt\\LaraExt\\Provider\\LaraExtServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=7.3",
    "laravel/framework": "^5.8|^6.0|^7.0|^8.0",
    "ext-pdo": "*",
    "mehr-it/buffer": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4|^8.4",
    "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0"
  }
}
